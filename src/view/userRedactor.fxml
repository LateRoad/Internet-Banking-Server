<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserRedactorController">
   <children>
      <TableView fx:id="tableViewUser" layoutX="14.0" layoutY="14.0" prefHeight="394.0" prefWidth="676.0">
        <columns>
          <TableColumn fx:id="loginColumn" prefWidth="132.0" text="Логин" />
          <TableColumn fx:id="passwordColumn" prefWidth="128.0" text="Пароль" />
            <TableColumn fx:id="nameColumn" prefWidth="115.0" text="Имя" />
            <TableColumn fx:id="surnameColumn" prefWidth="121.0" text="Фамилия" />
            <TableColumn fx:id="lastnameColumn" prefWidth="179.0" text="Отчество" />
        </columns>
      </TableView>
      <TextField fx:id="login" layoutX="165.0" layoutY="422.0" promptText="Логин" />
      <TextField fx:id="password" layoutX="165.0" layoutY="459.0" promptText="Пароль" />
      <TextField fx:id="name" layoutX="344.0" layoutY="422.0" promptText="Имя" />
      <TextField fx:id="surname" layoutX="344.0" layoutY="459.0" promptText="Фамилия" />
      <TextField fx:id="lastname" layoutX="523.0" layoutY="422.0" promptText="Отчество" />
      <Button fx:id="buttonAddUser" layoutX="523.0" layoutY="459.0" mnemonicParsing="false" onAction="#addUser" prefHeight="27.0" prefWidth="167.0" text="Добавить" />
      <Button fx:id="buttonToMain" layoutX="14.0" layoutY="459.0" mnemonicParsing="false" onAction="#showMain" prefHeight="27.0" prefWidth="35.0" text="&lt;" />
   </children>
</AnchorPane>
